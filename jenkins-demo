pipeline{
   agent any
     tools { 
        maven "default" 
    }   
stages{
       stage("Gitclonefromscm"){
           steps{
               git credentialsId: 'GITHUBCREDENTIALS', url: 'https://github.com/nageshanna/spring-boot-docker.git'
            }
        }
        stage("Maven Build") {
            steps{
               sh "mvn clean package"
            }
        }
}
}
