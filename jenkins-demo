pipeline{
   agent any
    export M2_HOME=/opt/maven
    export MAVEN_HOME=/opt/maven
    export PATH=${M2_HOME}/bin:${PATH}
     stages{
       stage("Gitclonefromscm"){
           steps{
               git credentialsId: 'GITHUBCREDENTIALS', url: 'https://github.com/nageshanna/spring-boot-docker.git'
            }
        }
        stage("Maven Build") {
            steps{
               sh "mvn clean package"
            }
        }
}
}
